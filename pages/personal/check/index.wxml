<view class="page">
  <view class="page__bd">
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">货物名称</view>
          <view class="new-preview__value">{{shipRecordDetails.commodityName}}</view>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">代理人</view>
          <view class="new-preview__value">{{shipRecordDetails.agentName}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">电话</view>
          <view class="new-preview__value">{{shipRecordDetails.agentMobile}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">编码</view>
          <view class="new-preview__value">{{shipRecordDetails.agentCode}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">外部编码</view>
          <view class="new-preview__value">{{shipRecordDetails.agentExternalCode}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">地址</view>
          <view class="new-preview__value">{{shipRecordDetails.agentAddress}}</view>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">生产厂商</view>
          <view class="new-preview__value">{{shipRecordDetails.manufacturer}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">价格</view>
          <view class="new-preview__value">{{shipRecordDetails.commodityPrice}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">单位</view>
          <view class="new-preview__value">{{shipRecordDetails.commodityUnit}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">重量</view>
          <view class="new-preview__value">{{shipRecordDetails.commodityWeight}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="new-preview__label__item">商品简介</view>
          <view class="new-preview__value">{{shipRecordDetails.commodityDescription}}</view>
        </view>
      </view>
    </view>
  </view>
</view>