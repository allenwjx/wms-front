<view class="page">
  <view class="page__bd">
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">货物名称</view>
          <view class="weui-form-preview__value_in-hd">{{shipRecordDetails.commodityName}}</view>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">代理人</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.agentName}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">电话</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.agentMobile}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">编码</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.agentCode}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">外部编码</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.agentExternalCode}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">地址</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.agentAddress}}</view>
        </view>
      </view>
      <view class="weui-form-preview__bd">
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">生产厂商</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.manufacturer}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">价格</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.commodityPrice}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">单位</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.commodityUnit}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">重量</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.commodityWeight}}</view>
        </view>
        <view class="weui-form-preview__item">
          <view class="weui-form-preview__label">商品简介</view>
          <view class="weui-form-preview__value">{{shipRecordDetails.commodityDescription}}</view>
        </view>
      </view>
      <view class="weui-form-preview__ft">
        <navigator url="" class="weui-form-preview__btn weui-form-preview__btn_primary" hover-class="weui-form-preview__btn_active">操作</navigator>
      </view>
    </view>
  </view>
</view>